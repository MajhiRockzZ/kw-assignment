<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data>

    <record id="stream_view_tree" model="ir.ui.view">
      <field name="name">Stream</field>
      <field name="model">kw_stream</field>
      <field name="arch" type="xml">
        <tree string="Stream">
          <field name="stream_name"/>
          <field name="stream_semester"/>
        </tree>
      </field>
    </record>

    <record id="stream_view_form" model="ir.ui.view">
      <field name="name">Stream</field>
      <field name="model">kw_stream</field>
      <field name="arch" type="xml">
        <form string="Stream">
          <sheet>
            <group>
              <field name="stream_name"/>
              <field name="stream_semester"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

  </data>
</odoo>
